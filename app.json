{
  "expo": {
    "name": "EndMyWallet",
    "slug": "endmywallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/images/wallet-icon.png",
    "scheme": "mywallet",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wallet-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mywallet.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wallet-icon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wallet-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "6e128876-30da-4814-9601-7437879e88e6"
      }
    }
  }
}
